	file = {Fichant - 1996 - Endommagement et anisotropie induite du béton de s.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/J5BIJIMA/Fichant - 1996 - Endommagement et anisotropie induite du béton de s.pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/UHCE3QXB/1996DENS0015.html:text/html;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/7F7LBJCJ/1996DENS0015.html:text/html}
