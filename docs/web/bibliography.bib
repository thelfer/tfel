	langid = {english},
	keywords = {Column collapse, Footing, Material point method, Shear-transformation-zone, Soil-pipe interaction},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/Z4RRW83R/S0266352X21001579.html:text/html}
}

