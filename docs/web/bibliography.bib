@article{wang_modelling_2023,
	title = {Modelling of the elastoplastic behaviour of the bio-cemented soils using an extended Modified Cam Clay model},
	issn = {1674-7755},
	url = {https://www.sciencedirect.com/science/article/pii/S1674775523003190},
	doi = {10.1016/j.jrmge.2023.09.015},
	abstract = {An elastoplastic constitutive model based on the Modified Cam Clay ({MCC}) model is developed to describe the mechanical behaviour of soils cemented via microbially induced calcite precipitation ({MICP}). It considers the increase of the elastic stiffness, the change of the yield surface due to {MICP} cementation and the degradation of calcium carbonate bonds during shearing. Specifically, to capture the typical contraction-dilation transition in {MICP} soils, the original volumetric hardening rule in the {MCC} model is modified to a combined deviatoric and volumetric hardening rule. The model could reproduce a series of drained triaxial tests on {MICP}-treated soils with different calcium carbonate contents. Further, we carry out a parametric study and observe numerical instability in some cases. In combination with an analytical analysis, our numerical modelling has identified the benefits and limitations of using {MCC}-based models in the simulation of {MICP}-cemented soils, leading to suggestions for further model development.},
	journaltitle = {Journal of Rock Mechanics and Geotechnical Engineering},
	shortjournal = {Journal of Rock Mechanics and Geotechnical Engineering},
	author = {Wang, Xuerui and Silbermann, Christian B. and Nagel, Thomas and Nackenhorst, Udo},
	urldate = {2024-04-05},
	date = {2023-12-13},
	keywords = {Contraction-dilation transition, Elastoplasticity, {MFront}, Microbially induced calcite precipitation ({MICP}), Modified cam clay ({MCC}), {OpenGeoSys}},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/EQ2N9KDJ/S1674775523003190.html:text/html},
}

@article{vedrine_role_2024,
	title = {Role of Ice Mechanics on Snow Viscoplasticity},
	volume = {51},
	rights = {© 2024. The Authors.},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2023GL107676},
	doi = {10.1029/2023GL107676},
	abstract = {The porous structure of snow becomes denser with time under gravity, primarily due to the creep of its ice matrix with viscoplasticity. Despite investigation of this behavior at the macroscopic scale, the driving microscopic mechanisms are still not well understood. Thanks to high-performance computing and dedicated solvers, we modeled snow elasto-viscoplasticity with 3D images of its microstructure and different mechanical models of ice. The comparison of our numerical experiments to oedometric compression tests measured by tomography showed that ice in snow rather behaves as a heterogeneous set of ice crystals than as homogeneous polycrystalline ice. Similarly to dense ice, the basal slip system contributed at most, in the simulations, to the total snow deformation. However, in the model, the deformation accommodation between crystals was permitted by the pore space and did not require any prismatic and pyramidal slips, whereas the latter are pre-requisite for the simulation of dense ice.},
	pages = {e2023GL107676},
	number = {7},
	journaltitle = {Geophysical Research Letters},
	author = {Védrine, Louis and Hagenmuller, Pascal and Gélébart, Lionel and Montagnat, Maurine and Bernard, Antoine},
	urldate = {2024-04-03},
	date = {2024},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2023GL107676},
	keywords = {crystal, microstructure, snow, tomography, viscoplasticity},
	file = {Full Text PDF:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/6KA363VX/Védrine et al. - 2024 - Role of Ice Mechanics on Snow Viscoplasticity.pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/PWL2836H/2023GL107676.html:text/html},
