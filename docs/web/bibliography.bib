	langid = {english},
	file = {Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/JEBX2JXN/333783199_Development_of_a_Novel_Damage_Model_for_Concrete_Subjected_to_High_Temperature_and_Co.html:text/html}
