@XMLOutputFile @xml_output@;
@ModellingHypothesis 'GeneralisedPlaneStrain';
@MaximumNumberOfSubSteps 1;

@Behaviour<generic> @library@ 'LogarithmicStrainNortonCreep' {
 tangent_operator : @tangent_operator@
};
@DrivingVariableEpsilon 1.e-10;
@StressEpsilon          2.e3;

@MaterialProperty<constant> 'YoungModulus'      80.e9;
@MaterialProperty<constant> 'PoissonRatio'       0.35;
@MaterialProperty<constant> 'A'   '0.003944e-6**4.39';
@MaterialProperty<constant> 'E'   4.39;

@ImposedDrivingVariable 'FYY' {0:1.,1.:4.5};
@ImposedDrivingVariable 'FXY' 0.;
@ImposedDrivingVariable 'FYX' 0.;

@ExternalStateVariable 'Temperature' 293.15;

@Times {0.,1 in 500};

@Test<file> @reference_file@ {'FYY':2,'FXX':3,'FZZ':3} 1e-5;
@Test<file> @reference_file@ {'SYY':4} 1.3e3;
@Test<file> @reference_file@ {'EquivalentViscoplasticStrain':5} 1e-4;
@Test<function> {'SZZ':'0','SXX':'0','SXY':'0'} 1.e3;
@Test<function> {'FXY': '0', 'FYX':'0'} 1.e-10;
