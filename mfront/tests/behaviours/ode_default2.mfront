@DSL DefaultModelDSL{
  automatic_declaration_of_the_temperature_as_first_external_state_variable :
      false
};
@Model ode_default2;
@Author Thomas Helfer;
@Date 21 / 09 / 2021;
@UseQt true;

@StateVariable real x;
@Parameter frequency A = 1.2;
@Parameter real theta = 0.5;

@Integrator{
  dx = -dt * A * x / (1 + A * theta * dt);
}
