@Author Thomas Helfer;
@Date 13/01/2018;
@Description{
  "This test checks that the temperature is correctly updated "
  "when sub-stepping is triggered."
};

@MaximumNumberOfSubSteps 1;
@Behaviour<umat> @library@ 'umattemperatureupdateonsubsteppingcheck';
@MaterialProperty<constant> 'YoungModulus'     150.e9;
@MaterialProperty<constant> 'PoissonRatio'       0.3;

@Real 'Ti' 293.15;
@Real 'Te' 1223.15;
@Real 'te' 3600;
@ExternalStateVariable<function> 'Temperature' 'Ti+(Te-Ti)*(t/te)';

@ImposedStrain<function> 'EXX' '1.e-3';

@Times {0.,'te' in 100};
