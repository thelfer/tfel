@Stress {-0.063203987691281,-0.010167747041919,809.76384761573,-0.01003418018283,-0.080032883309375,-0.1961210877778};

@ImposedStrain<evolution> 'EXX' {0:-0.0014848995463648,30000:-0.0019798303228701};
