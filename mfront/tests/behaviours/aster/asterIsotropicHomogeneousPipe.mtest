@Author Helfer Thomas;
@Date   09 avril 2013;
@Description{
  "Mod√©lisation d'un tube sous pression"
}

@ModellingHypothesis 'Axisymmetrical';
@Behaviour<aster> @library@ 'asterelasticity';
@MaterialProperty<constant> 'YoungModulus'     150.e9;
@MaterialProperty<constant> 'PoissonRatio'       0.3;

@ExternalStateVariable 'Temperature' {0:293.15,3600.:800};

// Phenix cladding radii
@Real Ri 2.825e-3;
@Real Re 3.250e-3;
@Real Rm '0.5*(Re+Ri)';
// inner and outer pressures
@Real 'Pi' {0:0,1:1.e5};
@Real 'Pe' {0:0,1:2.e5};

@Real 'K2' 'Ri**2*Re**2/(Re**2-Ri**2)*(Pe-Pi)';
@Real 'K1' '-Pi-K2/Ri**2';

@ImposedStrain<function> 'SRR' 'K1+K2/Rm**2';
@ImposedStrain<function> 'STT' 'K1+K2/Rm**2';
@ImposedStrain<function> 'SZZ' '1/(Re**2-Ri**2)*(Ri**2*Pi-Re**2*Pe)';

@Times {0.,1.};
