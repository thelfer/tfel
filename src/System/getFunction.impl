#if defined _WIN32 || defined _WIN64 ||defined __CYGWIN__
#define dlsym(handle, func) GetProcAddress (handle, func)
#else
#include<dlfcn.h>
#endif /* defined _WIN32 || _WIN64 || defined __CYGWIN__ */

#include"TFEL/System/getFunction.h"

