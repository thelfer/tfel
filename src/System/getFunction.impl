int tfel_getUnsignedShort(LibraryHandlerPtr lib,
			  const char * const name){
  unsigned short * n;
  n= (unsigned short *) dlsym(lib,name);
  if(n==0){
    return -1;
  }  
  return *n;
} 

int tfel_getBool(LibraryHandlerPtr lib,
		 const char * const name){
  unsigned short * n;
  n= (unsigned short *) dlsym(lib,name);
  if(n==0){
    return -1;
  }  
  return (int) *n;
}

char **
